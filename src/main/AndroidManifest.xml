<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.aniruddha.examples">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">

        <activity
            android:name="com.example.aniruddha.examples.MainActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <provider
            android:name="com.example.aniruddha.examples.SampleProvider"
            android:authorities="com.example.aniruddha.examples.data"
            android:enabled="true"
            android:exported="false" />

    </application>

</manifest>
